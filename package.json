  "description": "",
  "scripts": {
    "test:dev": "npm run clean:dev && cross-env NODE_ENV=development webpack --config webpack.dev.js && nodemon dist/server.js --ignore ./dist --ignore ./src/client",
    "test:prod": "npm run clean:prod && cross-env NODE_ENV=production webpack",
    "dev": "npm-run-all -p build:dev start:devTest",
    "start:dev": "cross-env NODE_ENV=development nodemon ./dist/server.bundle.js",
    "start:devTest": "cross-env NODE_ENV=development ts-node --project ./tsconfig.json ./src/server/index.ts",
    "start:prod": "cross-env NODE_ENV=production nodemon ./out/server.bundle.js",
    "build:prod": "npm run clean:prod && cross-env NODE_ENV=production webpack",
    "build:dev": "npm run clean:dev && cross-env NODE_ENV=development webpack --watch",
    "clean": "npm-run-all -p clean:*",
    "clean:dev": "rimraf dist",
    "clean:prod": "rimraf out",
    "unused": "npx depcheck"
  },
